Cell[TextData[{
 "Calculate the modal decomposition of the Teukolsky Source Term\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalT]", "lm\[Omega]"], " ", "=", " ", 
    RowBox[{"\[Integral]", 
     RowBox[{"d\[CapitalOmega]dt", " ", 
      SuperscriptBox["\[Rho]", 
       RowBox[{"-", "5"}]], 
      SuperscriptBox[
       OverscriptBox["\[Rho]", "_"], 
       RowBox[{"-", "1"}]], "2", "*", "\[ScriptCapitalT]", "*", 
      SubscriptBox[
       RowBox[{
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"-", "im\[Phi]"}], " ", "+", " ", "i\[Omega]t"}]], " "}], 
       RowBox[{"-", "2"}]], 
      SubsuperscriptBox["S", "lm", "a\[Omega]"]}]}]}], TraditionalForm]],
  InitializationCell->True,ExpressionUUID->
  "2ef9fdfc-abe6-4394-a736-df4197ab1bfb"]
}], "Text",
 CellChangeTimes->{{3.8675126777349873`*^9, 3.867512760789974*^9}, {
  3.867512879844679*^9, 3.867512884125413*^9}},
 Background->GrayLevel[
  0.85],ExpressionUUID->"2c531abd-4478-472b-ac46-7a8c9c859ab3"],


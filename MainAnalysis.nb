Cell[TextData[{
 "Calculate the asymptotic amplitude \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     SuperscriptBox["Z", "*"], "lm\[Omega]"], " ", "=", " ", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", 
       SubscriptBox[
        SuperscriptBox["i\[Omega]B", "inc"], "lm\[Omega]"]}]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["r", "+"], "\[Infinity]"], " ", 
      RowBox[{
       RowBox[{"\[DifferentialD]", "r"}], 
       FractionBox[
        RowBox[{" ", 
         RowBox[{
          SubscriptBox["\[ScriptCapitalT]", "lm\[Omega]"], 
          RowBox[{"(", "r", ")"}], " ", "*", " ", 
          RowBox[{
           SubsuperscriptBox["R", "lm\[Omega]", "in"], "(", "r", ")"}]}]}], 
        RowBox[{
         SuperscriptBox["\[CapitalDelta]", "2"], "(", "r", ")"}]]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"2e6fdd70-1241-4fd1-842a-f287e42a8bbf"],
